package basic06;

public class MessageBeanKr implements MessageBean{
	private String name;
	private String age;
	
	//1번. 생성자 방식
	public MessageBeanKr(String name, String age) {
		this.name = name;
		this.age = age;
	}
	
	public void sayHello() {
		System.out.println("제 이름은 " + name + "이고 나이는 " + age + "세입니다.");
	}


}
